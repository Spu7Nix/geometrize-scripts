def setupRotatedEllipse(shape) {
    var xBound = shape.m_model.getWidth();
    var yBound = shape.m_model.getHeight();

    shape.m_x = randomInRange(0, xBound);
    shape.m_y = randomInRange(0, yBound);
    shape.m_rx = randomInRange(1, 32);
    shape.m_ry = randomInRange(1, 32);
    shape.m_angle = randomInRange(0, 360);
}
