def mutateQuadraticBezier(shape) {
    var r = randomInRange(0, 2);
    var v = randomInVector(aoiPixels);
    
    switch(r) {
        case (0) {
            shape.m_cx = v.first;
            shape.m_cy = v.second;
            break;
        }
        case (1) {
            shape.m_x1 = v.first;
            shape.m_y1 = v.second;
            break;
        }
        case (2) {
            shape.m_x2 = v.first;
            shape.m_y2 = v.second;
            break;
        }
    }
}
